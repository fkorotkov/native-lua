{
  "env": {
    "includePaths": [
      "${workspaceFolder}/build",
      "${workspaceFolder}/src",
      "${workspaceFolder}/tests/ltests"
    ],
    "myCompilerPath": "/usr/local/bin/gcc-7"
  },
  "configurations": [
    {
      "name": "Mac",
      "intelliSenseMode": "${default}",
      "includePath": [
        "${includePaths}"
      ],
      "compilerPath": "/usr/bin/clang",
      "defines": [
        "LUA_USER_H=\"ltests.h\""
      ],
      "cStandard": "c11",
      "browse": {
        "path": [
          "${includePaths}"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    },
    {
      "name": "Linux",
      "intelliSenseMode": "${default}",
      "includePath": [
        "${includePaths}"
      ],
      "compilerPath": "/usr/bin/gcc",
      "defines": [
        "LUA_USE_LINUX",
        "LUA_USER_H=\"ltests.h\""
      ],
      "cStandard": "c11",
      "browse": {
        "path": [
          "${includePaths}"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    },
    {
      "name": "Win32",
      "intelliSenseMode": "${default}",
      "includePath": [
        "${includePaths}"
      ],
      "compilerPath": "/usr/bin/gcc",
      "defines": [
        "LUA_USER_H=\"ltests.h\""
      ],
      "cStandard": "c11",
      "browse": {
        "path": [
          "${includePaths}"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    }
  ],
  "version": 4
}
